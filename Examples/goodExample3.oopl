int main()
[
	int x, y;
	float z;
	string f;
	int mat[2][2];
]
{
	x = 1;
	z = x + 2.5;
	y = z;
	if (x == y) {
		while (x < 10) {
			x = x + 1;
			if (x == 5) {
				break;
			}
		}
	} elseif (x > 10 * 2 &&  y != 30) {
		print(x, y);
		for (int i = 0; i < 2; i = i + 1) {
			for (int j = 0; j < 2; j = j + 1) {
				read(f);
				mat[i][j] = f;
			}
		}
	}
}
